{
  "compilerOptions": {
    "strictNullChecks": true,
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "target": "es6",
    "lib": [
      "dom",
      "es2021"
    ],
    "skipLibCheck": true,
    "stripInternal": true,
    "baseUrl": "./",
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "dumi/theme/*": [
        ".dumi/theme/*"
      ],
    }
  },
  "exclude": [
    "node_modules"
  ]
}
